package com.example.ExerciceDataRest.model;

import java.util.List;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.OneToMany;
import javax.persistence.Table;

@Entity
@Table(name="professeur")
public class Professeur {
	
	@Id
	@GeneratedValue
	private long id;
	private String Nom;
	private String Prenom;
	private int NumeroIdentite;
	private String Telephone;
	private String Email;
	private int Niveau;
	@OneToMany(mappedBy="professeur")
    
	private List<Cours> cours;
	
	public Professeur() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Professeur(long id, String nom, String prenom, int numeroIdentite, String telephone, String email,
			int niveau, Cours cours) {
		super();
		this.id = id;
		Nom = nom;
		Prenom = prenom;
		NumeroIdentite = numeroIdentite;
		Telephone = telephone;
		Email = email;
		Niveau = niveau;
		this.cours = (List<Cours>) cours;
	}

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getNom() {
		return Nom;
	}

	public void setNom(String nom) {
		Nom = nom;
	}

	public String getPrenom() {
		return Prenom;
	}

	public void setPrenom(String prenom) {
		Prenom = prenom;
	}

	public int getNumeroIdentite() {
		return NumeroIdentite;
	}

	public void setNumeroIdentite(int numeroIdentite) {
		NumeroIdentite = numeroIdentite;
	}

	public String getTelephone() {
		return Telephone;
	}

	public void setTelephone(String telephone) {
		Telephone = telephone;
	}

	public String getEmail() {
		return Email;
	}

	public void setEmail(String email) {
		Email = email;
	}

	public int getNiveau() {
		return Niveau;
	}

	public void setNiveau(int niveau) {
		Niveau = niveau;
	}

	public Cours getCours() {
		return (Cours) cours;
	}

	public void setCours(Cours cours) {
		this.cours = (List<Cours>) cours;
	}

	@Override
	public String toString() {
		return "Professeur [id=" + id + ", Nom=" + Nom + ", Prenom=" + Prenom + ", NumeroIdentite=" + NumeroIdentite
				+ ", Telephone=" + Telephone + ", Email=" + Email + ", Niveau=" + Niveau + ", cours=" + cours + "]";
	}
	
	
	
	
	

}
