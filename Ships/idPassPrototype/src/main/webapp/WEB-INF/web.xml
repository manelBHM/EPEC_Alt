  </servlet>
  <servlet-mapping>
  	<servlet-name>servletMain</servlet-name>
  	<url-pattern>/servletMain</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>regServ</servlet-name>
  	<url-pattern>/regServ</url-pattern>
  </servlet-mapping>
