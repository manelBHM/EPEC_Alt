<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Flights</servlet-name>
  	<display-name>Flights</display-name>
  	<description></description>
  	<servlet-class>com.Servlet.Flights</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FlightServlet</servlet-name>
  	<display-name>FlightServlet</display-name>
