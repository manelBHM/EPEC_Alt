  </servlet>
  <servlet>
  	<servlet-name>EditFlServlet</servlet-name>
  	<display-name>EditFlServlet</display-name>
  	<description></description>
  	<servlet-class>com.Servlet.EditFlServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Flights</servlet-name>
  	<url-pattern>/Flights</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FlightServlet</servlet-name>
  	<url-pattern>/FlightServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditFlServlet</servlet-name>
  	<url-pattern>/EditFlServlet</url-pattern>
