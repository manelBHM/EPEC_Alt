package alternance.fizzbuzz;

public class FizzBuzz {

	
	public String nmbrAleatoir(int Min, int Max)
	{
		int nombreAleatoire = Min + (int)(Math.random() * ((Max - Min) + 1));
		
		String str = "";
		
		if(nombreAleatoire % 3 == 0)
			
		str = nombreAleatoire + " Fizz ";
		
		else if (nombreAleatoire % 5 == 0) 
			
			str = nombreAleatoire + " Buzz ";
		
		else if (nombreAleatoire % 3 == 0 && nombreAleatoire % 5 == 0)
			
			str = nombreAleatoire + " Fizz Buzz ";
		
		return str;
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
	}

}
