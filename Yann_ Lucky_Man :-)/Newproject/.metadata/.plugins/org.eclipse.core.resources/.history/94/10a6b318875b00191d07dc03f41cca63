package testifpair;

import static org.junit.jupiter.api.Assertions.*;
import org.junit.Assert.*;
import org.junit.*;

import org.junit.jupiter.api.Test;

import checkifpair.CheckIfPair;

class TestIfPair {

	CheckIfPair check = new CheckIfPair();
	
	@Test
	public final void Testpair(boolean c) {
		
		if(c == false)
		{
			System.out.println("ce nombre est impair ");
		}
		
		else
		{
			assertEquals(true, check.checkIfPair(0));
		}
		
	}

	

}
