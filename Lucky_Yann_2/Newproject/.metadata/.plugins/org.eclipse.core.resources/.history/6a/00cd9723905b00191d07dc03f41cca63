package my.TestUnitaire;

import junit.framework.TestCase;
import org.junit.Assert.*;
import org.junit.*;

public class TestClass extends TestCase {

	Calculator x = new Calculator();
	
	
	
	@Before
	public void setUp() throws Exception
	{
		System.out.println("Before Test @");
	}
	
	@After
	public void tearDown() throws Exception
	{
		System.out.println("After Test @");
	}
	
	@Test
	public final void testCarre()
	{
		
		System.out.println("Results for Test Carr√© : " + x.carre(5)); 
		assertEquals(3125, x.carre(5));
		
	}
	
	@Test
	public final void testCube()
	{
		System.out.println("Results for Test Cube : " + x.cube(8)); 
		assertEquals(512, x.cube(8));
	}
	
}
